# Local Development (No Redis/PostgreSQL)
APP_NAME="Spark Original UI"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:8000

# Database - SQLite (local)
DB_CONNECTION=sqlite
# DB_DATABASE will default to database/database.sqlite

# Cache & Sessions - File (local)
CACHE_STORE=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync

# Redis (not used in local mode)
REDIS_CLIENT=phpredis

# Mail
MAIL_MAILER=log

VITE_APP_NAME="${APP_NAME}"
